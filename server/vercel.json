{
	"version": 2,
	"builds": [
		{
			"src": "*.js",
			"use": "@vercel/node"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://solana-nf-tminter-client.vercel.app"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, POST"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "X-Requested-With, Content-Type, Accept"
				}
			]
		}
	]
}
